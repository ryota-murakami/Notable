{
  "name": "@notable/web",
  "version": "1.0.0",
  "description": "Notable web application built with Next.js",
  "private": true,
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "next dev --port 4378",
    "build": "next build",
    "export": "next build",
    "start": "next start --port 4378",
    "lint": "next lint --dir packages/web --fix",
    "lint:fix": "next lint --dir packages/web --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:jest": "jest",
    "e2e": "playwright test",
    "e2e:headed": "playwright test --headed",
    "e2e:debug": "playwright test --debug",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "deploy": "ts-node scripts/deploy.ts",
    "deploy:staging": "ENVIRONMENT=staging ts-node scripts/deploy.ts",
    "deploy:production": "ENVIRONMENT=production ts-node scripts/deploy.ts",
    "rollback": "ts-node scripts/rollback.ts",
    "docker:build": "docker build -t notable:latest .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f app",
    "clean": "rm -rf .next dist node_modules pnpm-lock.yaml"
  },
  "dependencies": {
    "@notable/sync": "workspace:*",
    "@radix-ui/themes": "^3.2.1",
    "@sentry/nextjs": "^8.47.0",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.48.0",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/typography": "latest",
    "@tanstack/react-query": "^5.83.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "framer-motion": "^12.23.11",
    "lucide-react": "^0.532.0",
    "next": "15.2.4",
    "next-themes": "^0.4.6",
    "posthog-js": "^1.257.0",
    "react": "^19",
    "react-dom": "^19",
    "slate": "^0.117.2",
    "slate-history": "^0.113.1",
    "slate-react": "^0.117.4",
    "sonner": "^2.0.6",
    "tailwind-merge": "^2.6.0",
    "web-vitals": "^5.0.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@playwright/test": "^1.50.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/chalk": "^2.2.4",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.13",
    "@types/node": "^24.1.0",
    "@types/prompts": "^2.4.9",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-window": "^1.8.8",
    "@types/speakeasy": "^2.0.10",
    "@types/stripe-v3": "^3.1.33",
    "@types/uuid": "^10.0.0",
    "@types/winston": "^2.4.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "chalk": "^5.4.1",
    "eslint": "^9.32.0",
    "eslint-config-next": "^15.4.3",
    "jest": "^29.7.0",
    "jsdom": "^26.1.0",
    "prompts": "^2.4.2",
    "supabase": "^1.206.1",
    "tailwindcss": "^4.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
