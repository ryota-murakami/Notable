{
  "name": "@notable/mobile",
  "version": "1.0.0",
  "description": "Notable mobile application built with React Native/Expo",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "echo 'Mobile build temporarily disabled - see Issue #14'",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "test": "jest",
    "e2e": "echo 'E2E tests for mobile'",
    "clean": "rm -rf .expo dist",
    "dev": "expo start --dev-client"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/stack": "^7.0.10",
    "@supabase/supabase-js": "^2.48.0",
    "expo": "~52.0.0",
    "expo-file-system": "^18.1.11",
    "expo-font": "^13.3.2",
    "expo-router": "^5.1.4",
    "expo-sharing": "^13.1.5",
    "expo-splash-screen": "^0.30.10",
    "expo-status-bar": "^2.2.3",
    "fuse.js": "^7.0.0",
    "react": "^19",
    "react-native": "0.76.6",
    "react-native-gesture-handler": "^2.27.2",
    "react-native-markdown-display": "^7.0.2",
    "react-native-paper": "^5.14.5",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "~4.4.0"
  },
  "devDependencies": {
    "@types/node": "^22",
    "@types/react": "^19",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "jest": "^30.0.0",
    "typescript": "^5"
  },
  "jest": {
    "preset": "../configs/jest-config/react-native"
  }
