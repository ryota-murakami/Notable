{
  "name": "@notable/mobile",
  "version": "1.0.0",
  "description": "Notable mobile application built with React Native/Expo",
  "//": "Trigger mobile e2e workflows for PR #177 - restart after infrastructure timeout",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build": "expo export --platform all --clear",
    "build:local": "expo export --platform all --dev --clear",
    "build:android": "EAS_NO_VCS=1 eas build --platform android --local --output=./builds",
    "build:ios": "EAS_NO_VCS=1 eas build --platform ios --local --output=./builds",
    "build:development": "eas build --profile development --platform all --non-interactive",
    "build:preview": "eas build --profile preview --platform all --non-interactive",
    "build:production": "eas build --profile production --platform all --non-interactive",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "e2e": "detox test",
    "e2e:build": "detox build",
    "e2e:build:ios": "detox build --configuration ios.sim.debug",
    "e2e:build:android": "detox build --configuration android.emu.debug",
    "e2e:test:ios": "detox test --configuration ios.sim.debug",
    "e2e:test:android": "detox test --configuration android.emu.debug",
    "e2e:clean": "detox clean-framework-cache && detox build-framework-cache",
    "clean": "rm -rf .expo dist node_modules pnpm-lock.yaml",
    "dev": "expo start --dev-client",
    "mobile:dev": "node scripts/mobile-dev.js",
    "mobile:ios": "node scripts/mobile-dev.js ios",
    "mobile:android": "node scripts/mobile-dev.js android",
    "mobile:reset": "node scripts/mobile-dev.js reset",
    "mobile:clean": "node scripts/mobile-dev.js clean",
    "mobile:debug": "node scripts/mobile-dev.js debug",
    "mobile:setup": "node scripts/setup-dev.js",
    "simulator:ios": "node scripts/ios-simulator.js setup",
    "simulator:ios:reset": "node scripts/ios-simulator.js reset",
    "simulator:ios:list": "node scripts/ios-simulator.js list",
    "emulator:android": "node scripts/android-emulator.js setup",
    "emulator:android:reset": "node scripts/android-emulator.js reset",
    "emulator:android:list": "node scripts/android-emulator.js list"
  },
  "dependencies": {
    "@babel/runtime": "^7.27.6",
    "@expo/vector-icons": "^14.1.0",
    "@notable/routing": "workspace:*",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/stack": "^7.0.10",
    "@supabase/supabase-js": "^2.48.0",
    "expo": "~52.0.0",
    "expo-file-system": "^18.0.12",
    "expo-font": "^13.3.2",
    "expo-linear-gradient": "~14.1.5",
    "expo-router": "^5.1.4",
    "expo-sharing": "^13.1.5",
    "expo-splash-screen": "^0.30.10",
    "expo-status-bar": "^2.2.3",
    "fuse.js": "^7.0.0",
    "react": "^19",
    "react-native": "0.76.6",
    "react-native-gesture-handler": "^2.27.2",
    "react-native-markdown-display": "^7.0.2",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "~4.4.0",
    "react-native-web": "~0.19.13"
  },
  "devDependencies": {
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.2.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "@types/react": "^19",
    "babel-preset-expo": "^13.2.3",
    "detox": "^20.40.2",
    "eas-cli": "^14.2.0",
    "eslint": "^9.32.0",
    "jest": "^29.7.0",
    "jest-expo": "^53.0.9",
    "jest-junit": "^16.0.0",
    "react-test-renderer": "^19.1.0",
    "sharp": "^0.34.3",
    "typescript": "^5.8.3"
  }
}
