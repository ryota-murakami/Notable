{
  "name": "notable-monorepo",
  "version": "1.0.0",
  "description": "A premium Notion clone with superior writing experience - Monorepo",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "volta": {
    "node": "22.17.1"
  },
  "scripts": {
    "build": "turbo build",
    "test": "turbo test",
    "lint": "turbo lint",
    "lint:fix": "turbo lint:fix",
    "typecheck": "turbo typecheck",
    "e2e": "turbo e2e",
    "dev": "turbo dev",
    "clean": "turbo clean && rm -rf node_modules pnpm-lock.yaml",
    "prettier": "prettier --ignore-unknown --write .",
    "sherif:fix": "pnpm sherif --fix -r unsync-similar-dependencies",
    "knip": "knip",
    "deps:dedupe": "pnpm dedupe",
    "deps:cleanup": "pnpm dedupe && pnpm install --force",
    "security:audit": "pnpm audit",
    "security:fix": "pnpm audit --fix"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "eslint": "^9.32.0",
    "husky": "^9.1.7",
    "knip": "5.62.0",
    "lint-staged": "^16.1.2",
    "prettier": "^3.6.2",
    "sherif": "^1.6.1",
    "turbo": "^2.5.5",
    "typescript": "^5.8.3"
  },
  "packageManager": "pnpm@10.13.1"
}